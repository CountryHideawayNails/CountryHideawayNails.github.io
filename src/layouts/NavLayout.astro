---
import RootLayout, { type RootLayoutProps } from "./RootLayout.astro";

import NavBar from "../components/core/NavBar.astro";
import Footer from "../components/core/Footer.astro";

export interface NavLayoutProps extends RootLayoutProps {};

const { page_title } = Astro.props as NavLayoutProps;
---

<RootLayout page_title={page_title}>
    <NavBar />
    <slot />
    <Footer />
</RootLayout>